# OpenAI API key (required for GPT-4o frame descriptions and DALL-E if selected)
OPENAI_API_KEY=your_openai_api_key_here

# Replicate API token (required for Stable Diffusion if selected)
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Stable Diffusion inpainting model (optional, default is a good inpainting model)
# SD_INPAINT_MODEL=stability-ai/stable-diffusion:27b93a2413e7f36cd83da926f3656280b2931564ff050bf9575f1fdf9bcd7478

# Directory containing video files (default: ./videos)
VIDEO_DIRECTORY=./videos

# Default maximum number of frames to generate from query (default: 5)
MAX_FRAMES_DEFAULT=5

# Default number of top results to return (default: 3)
TOP_K_DEFAULT=3

# Debug settings
DEBUG_MODE=false
DEBUG_LOG_DIR=./logs/debug
DEBUG_FRAMES_DIR=./logs/frames